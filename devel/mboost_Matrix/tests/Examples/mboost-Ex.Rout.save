            nX <- newX(x = newdata[[xname]], z = newdata[[zname]])
            nX %*% coef
        }
        ret <- list(model = coef, predict = predictfun, fitted = function() X %*% 
            coef)
        class(ret) <- c("basefit", "baselm")
        ret
    }
    ret <- list(fit = fitfun, hatmatrix = function() X %*% Xsolve)
    class(ret) <- "basisdpp"
    ret
}
